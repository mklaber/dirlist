{
  "query": {
    "filtered": {
      "filter": {
        "and": {
          "filters": [
            {
              "term" : {
                "file_extension" : "mp4"
              }
            },
            {
              "range" : {
                "size" : {
                  "gte" : 100000000
                }
              }
            },
            {
              "range" : {
                "date_modified": {
                  "lt" : "2014-01-01"
                }
              }
            },
            {
              "term" : {
                "path.tree" : "/Users/mklaber/Pictures"
              }
            }
          ]
        }
      }
    }
  },
  "aggs": {
    "file_size_saved": {
      "sum": { "field" : "size"}
    }
  }
}